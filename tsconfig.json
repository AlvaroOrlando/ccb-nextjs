{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"],
      "@/styles/*": ["./src/styles/*"],
      "@/public/*": ["./src/public/*"],
      "@/hooks/*": ["./src/hooks/*"],
      "@/scss/*": ["./src/scss/*"],
      "@/assets/*": ["./src/assets/*"],
      "@/utilities/*": ["./src/utilities/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx",  "src/lib/db.ts", "src/pages/api/clubecad/getUserByCpf.js"],
  "exclude": ["node_modules"]
}
